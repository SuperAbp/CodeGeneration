@using AbpCodeGeneration.VisualStudio.Common.Model
@inherits RazorEngine.Templating.TemplateBase<CreateFileInput>

namespace @(Model.Namespace).@(Model.DirectoryName).Dto
{
    public class @(Model.ClassName)CreateOrUpdateDtoBase
    {
    @foreach (DtoPropertyInfo classProperty in Model.PropertyInfos)
    {
        if (classProperty.IsEdit)
        {
        <text>public @Raw(classProperty.PropertyType) @Raw(classProperty.PropertyName) { get; set; }</text>
        }
    }
    }
}