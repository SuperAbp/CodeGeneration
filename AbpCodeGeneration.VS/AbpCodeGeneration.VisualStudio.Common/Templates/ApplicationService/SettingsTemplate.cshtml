@using AbpCodeGeneration.VisualStudio.Common.Model
@inherits RazorEngine.Templating.TemplateBase<CreateFileInput>
namespace @Model.Namespace@(String.IsNullOrEmpty(Model.Prefix) ? "" : Model.Prefix).@Model.DirectoryName
{
    /// <summary>
    /// <see cref="@(Model.ClassName)Settings"/>
    /// </summary>
    public class @(Model.ClassName)Settings
    {
        /// <summary>
        /// 分组名称
        /// </summary>
        public const string GroupName = "@(Model.ClassName)s";

        /// <summary>
        /// 每页最大条数
        /// </summary>
        public const string MaxPageSize = GroupName + ".MaxPageSize";
    }
}
